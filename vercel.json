{
    "builds": [
      {
        "src": "insurancemanagement/wsgi.py",
        "use": "@vercel/python"
      },
      {
        "src": "build.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "staticfiles"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "insurancemanagement/wsgi.py"
      }
    ],
    "outputDirectory": "staticfiles"
}